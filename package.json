{
  "name": "keadatabase",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://beta.keadatabase.nz",
  "dependencies": {
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.0.3",
    "react-redux": "^5.0.5",
    "react-router-bootstrap": "^0.24.2",
    "react-router-dom": "^4.1.1",
    "react-truncate": "^2.1.4",
    "redux": "^3.7.1",
    "redux-form": "^6.8.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "bootstrap-sass": "^3.3.7",
    "node-sass": "^4.5.2",
    "react-scripts": "0.9.5",
    "s3-deploy": "^0.7.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "pretest": "npm run build-css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "predeploy": "npm run build-css && npm run build",
    "deploy": "s3-deploy './build/**' --cwd './build/' --region ap-southeast-2 --bucket beta.keadatabase.nz"
  }
}
