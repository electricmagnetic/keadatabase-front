{
  "name": "keadatabase-front",
  "version": "2.0.0-alpha",
  "private": true,
  "homepage": "https://beta.keadatabase.nz",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.1.1",
    "formik": "^1.0.1",
    "mapbox-gl": "^0.45.0",
    "moment": "^2.22.2",
    "normalizr": "^3.2.4",
    "prop-types": "^15.6.1",
    "qs": "^6.5.2",
    "react": "^16.4.0",
    "react-bootstrap-typeahead": "^3.1.5",
    "react-datepicker": "^1.5.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-mapbox-gl": "^3.8.0",
    "react-moment": "^0.7.7",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "react-truncate": "^2.3.0",
    "redux": "^4.0.0",
    "redux-api-middleware": "^2.3.0",
    "redux-thunk": "^2.3.0",
    "yup": "^0.25.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules --include-path ./src/assets/css src/ -o src/",
    "watch-css": "node-sass-chokidar --include-path ./node_modules --include-path ./src/assets/css src/ -o src/ --watch --recursive",
    "pretest": "npm run build-css",
    "predeploy": "npm run build-css && npm run build",
    "deploy": "s3-deploy './build/**' --cwd './build/' --region ap-southeast-2 --bucket beta.keadatabase.nz",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "s3-deploy": "^1.0.0",
    "node-sass-chokidar": "^1.3.0"
  }
}
